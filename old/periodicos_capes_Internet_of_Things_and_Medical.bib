number = "6",
year = "2016",
title = "Wearables and Apps -- Modern Diagnostic Frameworks for Health Promotion through Sport.",
language = "eng",
author = "Baca, A.",
keywords = "PHYSIOLOGICAL aspects of sports ; WEARABLE technology ; PHYSICAL fitness mobile apps ;",
month = "June",
}

