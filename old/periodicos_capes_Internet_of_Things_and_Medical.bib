language = "eng",
author = "Lao, Xiangda and Zhang, Jing and Bai, Chunxue",
keywords = "Delivery of Health Care, Integrated; Disease Management; Health Services Accessibility; Telemedicine; Pulmonary Disease, Chronic Obstructive -- Diagnosis; Pulmonary Disease, Chronic Obstructive -- Therapy",
