keywords = "Security ; Integrity ; Emergencies ; Patients ; Health ; Design Analysis ; Cloud Computing ; Internet ; Information Systems (General) (Ci) ; Remote Data Integrity Checking ; Public Cloud ; Data Sharing",
month = "July",
url = "http://search.proquest.com/docview/1855377016/",
