language = "eng",
address = "Basel",
author = "Eskofier, Bjoern and Lee, Sunghoon and Baron, Manuela and Simon, Andre and Martindale, Christine and Gaßner, Heiko and Klucken, Jochen",
keywords = "Promotion ; Internet ; Health Promotion ; Internet ; Gait ; Medical Research ; Internet ; Health Care ; Mobility ; Wearable ; Gait ; Health Care ; Quality of Life ; Diagnostic Systems ; Transformation ; Occupational Health ; Clothing ; Internet ; Quality of Life ; Mobility",
month = "January",
url = "http://search.proquest.com/docview/1965673743/?pq-origsite=primo",
}

@article{DricotJean-Michel2011Iote,
issn = "1687-1499",
abstract = "Personal area networks and, more specifically, body area networks (BANs) are key building blocks of future generation networks and of the Internet of Things as well. In this article, we present a novel analytical framework for network performance analysis of body sensor networks with hierarchical (tree) topologies. This framework takes into account the specificities of the on-body channel modeling and the impact of the surrounding environment. The obtained results clearly highlight the differences between indoor and outdoor scenarios, and provide several insights on BAN design and analysis. In particular, it will be shown that the BAN topology should be selected according to the foreseen medical application and the deployment environment.",
journal = "EURASIP Journal on Wireless Communications and Networking",
pages = "1--17",
