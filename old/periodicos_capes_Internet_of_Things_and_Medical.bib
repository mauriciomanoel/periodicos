keywords = "Policies ; Guidelines ; Sustainable Development ; Information Technology ; Governments ; Utilization ; Sustainability ; Legislation ; Environmental Engineering (General) (CE) ; Green It ; Big Data ; Delphi Technique ; Green Growth ; Sustainable Development Green It ; Sustainable Development",
