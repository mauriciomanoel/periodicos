keywords = "United States–Us ; Receivers & Amplifiers ; Standards ; Internet of Things ; Noise ; Spectrum Allocation ; Radios ; Antennas ; Wireless Access Points",
month = "June",
url = "http://search.proquest.com/docview/1916318120/?pq-origsite=primo",
