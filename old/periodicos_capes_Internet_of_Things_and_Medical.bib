journal = "Ethics and Information Technology",
pages = "157--175",
volume = "19",
publisher = "Springer Netherlands",
number = "3",
year = "2017",
title = "Ethics of the health-related internet of things: a narrative review",
language = "eng",
address = "Dordrecht",
author = "Mittelstadt, Brent",
