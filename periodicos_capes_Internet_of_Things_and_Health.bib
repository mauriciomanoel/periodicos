author = "Alagarsamy, Sumithra and Rajagopalan, S",
keywords = "Encryption ; Wireless Networks ; Wireless Networks ; Cryptography ; Authenticity ; Computer Science ; Security ; Network Security ; Digital Signatures ; Communication ; Public Key Infrastructure ; Confidentiality ; Distributing ; Computational Efficiency ; Internet of Things ; Access Control ; Communications Networks ; Design ; Computer Applications ; Cryptography ; Differential Equations ; Privacy ; Research & Development–R&D ; Mobile Communication Systems ; Security ; Cryptography ; Differential Equations ; Internet ; Algorithms ; Algebraic Geometry ; Encryption ; Network Analysis ; Research Integrity",
month = "October",
url = "http://search.proquest.com/docview/1952054894/?pq-origsite=primo",
