pages = "7076--7083",
volume = "63",
publisher = "IEEE",
number = "11",
year = "2016",
title = "Enhanced Restricted Boltzmann Machine With Prognosability Regularization for Prognostics and Health Assessment",
language = "eng",
address = "USA",
author = "Liao, Linxia and Jin, Wenjing and Pavel, Radu",
keywords = "Feature Extraction ; Prediction Algorithms ; Degradation ; Algorithm Design and Analysis ; Prognostics and Health Management ; Industries ; Training ; Restricted Boltzmann Machine (Rbm) ; Deep Learning ; Feature Extraction ; Prognostics and Health Management (Phm) ; Regularization ; Remaining Useful Life (Rul) Prediction ; Power, Energy and Industry Applications ; Signal Processing and Analysis ; Communication, Networking and Broadcast Technologies",
month = "November",
}

