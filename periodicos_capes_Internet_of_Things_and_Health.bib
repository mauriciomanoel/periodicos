keywords = "United Kingdom–UK ; Consortia ; Audiences ; Cedar ; Internet of Things ; Consortia ; Radio Networks ; Audiences ; Users ; Foresight ; Internet of Things ; Iot ; Future Technologies",
month = "December",
url = "http://search.proquest.com/docview/1974585813/?pq-origsite=primo",
}

