publisher = "Elsevier BV",
year = "2017",
title = "Intelligent homes’ technologies to optimize the energy performance for the net zero energy home",
language = "eng",
address = "Lausanne",
author = "Alfaris, Fadi and Juaidi, Adel and Manzano-Agugliaro, Francisco",
keywords = "Building Automation ; Monitoring Systems ; Energy Management ; Energy Consumption ; Green Buildings ; Energy Management ; Information Technology ; Internet ; Intelligence ; Sensors ; Applications Programs ; Measuring Instruments ; Demand ; Health Care ; Mobile Computing ; Sensors ; Smart Buildings ; Automation ; Renewable Energy ; Monitoring ; Energy Use ; Energy Demand ; Electricity Consumption ; Residential Energy ; Case Studies ; Villas ; Renewable Energy ; Internet ; Buildings ; Information Technology ; Information Processing ; System Effectiveness ; Building Management Systems ; Energy Consumption ; Supply and Demand ; Internet ; New Technology ; Green Buildings",
month = "October",
url = "http://search.proquest.com/docview/1969923327/?pq-origsite=primo",
