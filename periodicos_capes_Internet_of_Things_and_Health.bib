title = "An integrated systems approach to plateau ecosystem management--a scientific application in Qinghai and Tibet plateau",
language = "eng",
address = "New York",
author = "Liu, Fang and Bi, Zhuming and Xu, Eric and Ga, Qin and Yang, Quanyu and Yang, Yingzhong and Ma, Lan and Wuren, Tana and Ge, Rili",
keywords = "Systems Integration ; Health Informatics ; Transducers ; Ecosystems ; Environmental Management ; Gene Expression ; Analysis ; Studies ; Cloning ; Communications & Information Management ; Management Science/Operations Research ; Experimental/Theoretical",
month = "April",
url = "http://search.proquest.com/docview/1664982389/?pq-origsite=primo",
