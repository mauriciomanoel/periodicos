pages = "n/a--n/a",
volume = "29",
number = "20",
year = "2017",
title = "A stochastic game net‐based model for effective decision‐making in smart environments",
author = "Sandhu, Rajinder and Sood, Sandeep K.",
keywords = "Stochastic Game Net ; Gerontechnology ; Game Theory ; Internet Of Things Iot ; Smart Home",
month = "October",
