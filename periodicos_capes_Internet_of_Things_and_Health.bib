volume = "8",
publisher = "Nature Publishing Group",
year = "2017",
title = "A microprocessor based on a two-dimensional semiconductor",
language = "eng",
address = "London",
author = "Wachter, Stefan and Polyushkin, Dmitry and Bethge, Ole and Mueller, Thomas",
keywords = "Condensed Matter - Mesoscale And Nanoscale Physics;",
