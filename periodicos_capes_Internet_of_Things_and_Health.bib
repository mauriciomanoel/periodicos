keywords = "Studies ; Business Intelligence Software ; Queries ; Process Management ; Internet ; Business Process Management ; Abundance ; Repositories ; Industrial Applications ; Information Management ; Decision Making ; Motivation ; Coding ; Intelligence (Information) ; Literature Reviews ; Deduction (Ci)",
month = "August",
