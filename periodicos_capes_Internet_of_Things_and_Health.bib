language = "eng",
address = "Oxford",
author = "Aktaruzzaman, Md and Rivolta, Massimo and Karmacharya, Ruby and Scarabottolo, Nello and Pugnetti, Luigi and Garegnani, Massimo and Bovi, Gabriele and Scalera, Giovanni and Ferrarin, Maurizio and Sassi, Roberto",
keywords = "Research ; Laboratories ; Feature Extraction ; Chest ; Accuracy ; Process Controls ; Classification ; Electrodes ; Wearable ; Heart Rate ; Sleep and Wakefulness ; Internet of Things ; Wrist ; EKG ; Classification ; Sleep ; Electroencephalography ; Statistical Analysis ; Accelerometers ; Sleep Disorders ; Variability ; Quality ; Heart Rate ; Redundancy ; Gene Expression ; Support Vector Machines ; Sleep Disorders ; Heart Rate ; Studies ; Classification ; Sensors ; Optimization ; Wrist ; Classification ; Sleep ; Chest ; Heart Rate ; Sleep Scoring ; Heart Rate Variability ; Actigraphy ; Svm Classifier ; Wearable Sensors",
month = "October",
url = "http://search.proquest.com/docview/1954364213/?pq-origsite=primo",
}

@article{AdameToni2018CARh,
